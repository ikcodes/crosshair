extends ../layouts/default

block content
	.black-bg.grid-container: .grid-x.align-center: .cell.large-8.medium-10#influencer-registration
		// Begin Mailchimp Signup Form
		#mc_embed_signup
			form#mc-embedded-subscribe-form.validate(action='https://gyrosityprojects.us12.list-manage.com/subscribe/post?u=caae0a7b1ec989477bc61346a&id=91e002ae96' method='post' name='mc-embedded-subscribe-form' target='_blank' novalidate='')
				#mc_embed_signup_scroll
					.hero-text
						h1.caps.text-center Become an influencer
							hr
					.indicates-required
						span.asterisk *
						|  indicates required
					.mc-field-group
						label(for='mce-EMAIL')
							| Email Address  
							span.asterisk *
						input#mce-EMAIL.required.email(type='email' value='' name='EMAIL')
					.mc-field-group
						label(for='mce-FNAME')
							| First Name  
							span.asterisk *
						input#mce-FNAME.required(type='text' value='' name='FNAME')
					.mc-field-group
						label(for='mce-LNAME') Last Name 
						input#mce-LNAME(type='text' value='' name='LNAME')
					.mc-field-group
						label(for='mce-SELECTYUI') Spotify Playlist Link 
						input#mce-SELECTYUI(type='text' value='' name='SELECTYUI')
					.mc-field-group
						label(for='mce-TEXTYUI_3') Additional Spotify Link 
						input#mce-TEXTYUI_3(type='text' value='' name='TEXTYUI_3')
					.mc-field-group
						label(for='mce-TEXT3') YouTube Channel Link 
						input#mce-TEXT3(type='text' value='' name='TEXT3')
					.mc-field-group
						label(for='mce-SELECT3') Instagram Account Name 
						input#mce-SELECT3(type='text' value='' name='SELECT3')
					.mc-field-group
						label(for='mce-TEXT4') Additional Link 
						input#mce-TEXT4(type='text' value='' name='TEXT4')
					.mc-field-group
						label(for='mce-TEXT5') How did you hear about Crosshair? 
						input#mce-TEXT5(type='text' value='' name='TEXT5')
					#mce-responses.clear
						#mce-error-response.response(style='display:none')
						#mce-success-response.response(style='display:none')
					// real people should not fill this in and expect good things - do not remove this or risk form bot signups
					div(style='position: absolute; left: -5000px;' aria-hidden='true')
						input(type='text' name='b_caae0a7b1ec989477bc61346a_91e002ae96' tabindex='-1' value='')
					.clear
						input#mc-embedded-subscribe.button(type='submit' value='Submit' name='subscribe')
		script(type='text/javascript' src='//s3.amazonaws.com/downloads.mailchimp.com/js/mc-validate.js')
		script(type='text/javascript').
			(function($) {window.fnames = new Array(); window.ftypes = new Array();fnames[0]='EMAIL';ftypes[0]='email';fnames[1]='FNAME';ftypes[1]='text';fnames[2]='LNAME';ftypes[2]='text';fnames[5]='SELECTYUI';ftypes[5]='text';fnames[3]='TEXTYUI_3';ftypes[3]='text';fnames[6]='TEXT3';ftypes[6]='text';fnames[7]='SELECT3';ftypes[7]='text';fnames[8]='TEXT4';ftypes[8]='text';fnames[9]='TEXT5';ftypes[9]='text';}(jQuery));var $mcj = jQuery.noConflict(true);
		// End mc_embed_signup
